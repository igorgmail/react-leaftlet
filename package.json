{
  "name": "leaflet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "build": "env-cmd -f .env.production react-scripts build",
    "format:check": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:check": "eslint --fix --max-warnings 0 'src/**/*.{js,jsx,ts,tsx}'",
    
    "update-i18n": "env-cmd -f .env.local node update-i18n.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/x-date-pickers": "^6.12.1",
    "@react-leaflet/core": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/leaflet": "^1.9.3",
    "@types/leaflet-routing-machine": "^3.2.4",
    "@types/papaparse": "^5.3.7",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.9",
    "env-cmd": "^10.1.0",
    "google-spreadsheet": "^3.2.0",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^13.0.1",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-redux": "^8.1.1",
    "react-resizable-layout": "^0.6.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.18.37",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-leaflet": "^3.0.0",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^9.0.4",
    "commitlint": "^17.6.6",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^4.4.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "no-console": "warn"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g commitlint.js",
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "*.md": "prettier --write"
  },
  "prettier": {
    "endOfLine": "lf",
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "singleQuote": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
