{
  "name": "leaflet-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "build": "env-cmd -f .env.production react-scripts build",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "format:check": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:check": "eslint --fix --max-warnings 0 'src/**/*.{js,jsx,ts,tsx}'",
    "update-i18n": "env-cmd -f .env.local node update-i18n.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@react-leaflet/core": "^2.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/leaflet": "^1.9.3",
    "axios": "^1.5.1",
    "env-cmd": "^10.1.0",
    "leaflet": "^1.9.4",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet-routing-machine": "^3.2.12",
    "luxon": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-leaflet-custom-control": "^1.4.0",
    "react-redux": "^8.1.1",
    "react-resizable-layout": "^0.6.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.5",
    "@types/luxon": "^3.3.4",
    "@types/node": "^16.18.37",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-leaflet": "^3.0.0",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^9.0.4",
    "eslint": "^8.43.0",
    "typescript": "^4.4.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "no-console": "warn"
    }
  },

  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ],
    "*.md": "prettier --write"
  }
}
